    "next": "12.1.0",
    "next-contentlayer": "^0.0.34",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-responsive-carousel": "^3.2.22",
    "sharp": "^0.30.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/bundle-analyzer": "^12.0.9",
    "@svgr/core": "^6.2.0",
    "@svgr/webpack": "^6.2.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
