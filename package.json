  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/appenin/appenin.fr"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "clean": "rimraf .next",
    "dev": "yarn clean && next dev",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
